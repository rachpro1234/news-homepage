{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "css-prefix-order": "off",
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "height: fit-content",
          "max-height: fit-content",
          "width: fit-content"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "button-type": "off",
    "disown-opener": "off"
  }
}